{
  "name": "blog-test-app",
  "description": "Blog test application using Next.js",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Ivan Vasilev",
    "url": "https://github.com/IvanVasil-ev"
  },
  "engines": {
    "node": ">=14.17.4",
    "yarn": ">=1.22.11"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
